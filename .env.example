# Токен от BotFather (для Телеграма)
BOT_TOKEN=your_bot_token_here

# ID канала для автопостинга (получить через @userinfobot)
CHANNEL_ID=-1001234567890

# Ключ от OpenRouter (начинается на sk-or-...)
OPENROUTER_API_KEY=your_openrouter_key_here

# Имя модели (DeepSeek через OpenRouter)
MODEL_NAME=deepseek/deepseek-chat
# Если хочешь использовать R1 (Reasoning model), пиши: deepseek/deepseek-r1
